    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "echo 'No tests configured'",
    "commit": "cz",
    "prepare": "husky"
  },
  "dependencies": {
