    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "lucide-react": "^0.453.0",
    "next": "^14.2.25",
    "next-intl": "^3.21.1",
    "next-themes": "^0.3.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "resend": "^4.6.0",
    "sharp": "^0.33.5",
    "stripe": "^16.12.0",
    "svix": "^1.68.0",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
